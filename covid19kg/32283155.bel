##################################################################################
# Statements #
##################################################################################

SET Citation = {"PubMed", "32283155"}
SET PublicationType = "Research"
SET Section = "Results"
SET Species = "9606"
SET Custom_Disease = "COVID-19"

SET Evidence = "On admission, lymphocytopenia was present in 83.2% of the patients, thrombocytopenia in 36.2%, and leukopenia in 33.7%"

path(DO:"COVID-19") -- path(MESH:"T-Lymphocytopenia")
path(DO:"COVID-19") -- path(DO:thrombocytopenia)
path(DO:"COVID-19") -- path(DO:leukopenia)

SET Evidence = "Most of the patients had elevated levels of C-reactive protein; less common were elevated levels of alanine aminotransferase, aspartate aminotransferase, creatine kinase, and d-dimer."

SET MeSHAnatomy = "Blood"
a(MESH:"C-Reactive Protein") pos path(DO:"COVID-19")
p(HGNC:GPT) pos path(DO:"COVID-19")
p(HGNC:GOT1) pos path(DO:"COVID-19")
p(HGNC:GOT2) pos path(DO:"COVID-19")
p(HGNC:CKM) pos path(DO:"COVID-19")
bp(EFO:"D dimer measurement") pos path(DO:"COVID-19")
UNSET MeSHAnatomy

SET Evidence = "A majority of the patients (58.0%) received intravenous antibiotic therapy, and 35.8% received oseltamivir therapy; oxygen therapy was administered in 41.3% and mechanical ventilation in 6.1%; higher percentages of patients with severe disease received these therapies (Table 3)."

a(CHEBI:oseltamivir) -| path(DO:"COVID-19")
bp(MESH:"Oxygen Inhalation Therapy") -| path(DO:"COVID-19")
bp(MESH:Ventilation) -| path(DO:"COVID-19")

SET Evidence = "Systemic glucocorticoids were given to 204 patients (18.6%), with a higher percentage among those with severe disease than nonsevere disease (44.5% vs. 13.7%)."
a(CHEBI:glucocorticoid) -| path(DO:"COVID-19")

SET Evidence = "During hospital admission, most of the patients received a diagnosis of pneumonia from a physician (91.1%), followed by ARDS (3.4%) and shock (1.1%)."
path(DO:"COVID-19") -- path(DO:pneumonia)
path(DO:"COVID-19") -- path(MESH:Shock)
path(DO:"COVID-19") -- path(EFO:"acute respiratory distress syndrome")
UNSET Custom_Disease
UNSET Species
UNSET Section
UNSET PublicationType