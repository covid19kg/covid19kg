##################################################################################
# Statements #
##################################################################################
SET Citation = {"DOI", "https://www.biorxiv.org/content/10.1101/2020.04.14.041228v1.full.pdf",  "https://doi.org/10.1101/2020.04.14.041228"}
SET PublicationType = "Research"
SET PublicationStatus = "Preprint"
SET Section = "Results"

SET Species = "9606"
SET Custom_Disease = "COVID-19"
SET CellLine = "Huh7 cell"
SET BioAssay = "reverse transcription PCR"
SET Evidence = "As depicted in Figure 1, treatment of cells with auranofin resulted in a 70% reduction in the viral RNA in the supernatant compared to the DMSO at 24 hours after infection. At 48 hours, there was an 85% reduction in the viral RNA in thesupernatant compared to the DMSO."
SET Timeline = {"24hr", "48hr"}
SET Sample = "supernatant"
a(CHEBI:auranofin) -| a(TAX:"Severe acute respiratory syndrome coronavirus 2")
UNSET Sample
SET Evidence = "Similarly, the levels of intracellular viral RNA decreased by 85% at 24 hours and 95% at 48 hours in auranofin-treated cells compared to the DMSO-treated cells."
SET Sample = "intracellular"
a(CHEBI:auranofin) -| a(TAX:"Severe acute respiratory syndrome coronavirus 2")
UNSET Sample

SET Evidence = " Auranofin inhibited virus replication in the infected cells at EC50 of approximately 1.5 µM. "

a(CHEBI:auranofin) -| bp(GO:"viral genome replication")

SET Evidence = "SARS-COV-2 infection induces a strong up-regulation of IL-6, IL-1β, TNFα and NF-kB in Huh7 cells (Figure 3). Treatment with auranofin dramatically reduced the expression of SARS-COV-2-induced cytokines in Huh7 cells"

a(TAX:"Severe acute respiratory syndrome coronavirus 2") -> r(HGNC:IL6)
a(TAX:"Severe acute respiratory syndrome coronavirus 2") -> r(HGNC:IL1B)
a(TAX:"Severe acute respiratory syndrome coronavirus 2") -> r(HGNC:TNF)
a(TAX:"Severe acute respiratory syndrome coronavirus 2") -> r(HGNC:NFKB1)
a(TAX:"Severe acute respiratory syndrome coronavirus 2") -> r(HGNC:NFKB2)
a(CHEBI:auranofin) -| r(HGNC:IL6)
a(CHEBI:auranofin) -| r(HGNC:IL1B)
a(CHEBI:auranofin) -| r(HGNC:TNF)
a(CHEBI:auranofin) -| r(HGNC:NFKB1)
a(CHEBI:auranofin) -| r(HGNC:NFKB1)
UNSET Timeline
SET Evidence = "We also demonstrate that auranofin treatment resulted in significant reduction in virus-induced inflammation."

a(CHEBI:auranofin) -| bp(GO:"inflammatory response")
UNSET CellLine
UNSET BioAssay
UNSET Species
UNSET Custom_Disease
UNSET Section
UNSET PublicationStatus
UNSET PublicationType
